#include<cstdio>
#include<bitset>
using namespace std;

namespace TYC
{
    bitset<2000010> f;

    void work()
    {
		int n,x,sum=0,ans=0;
		scanf("%d",&n);
		f[0]=1;
		for(int i=1;i<=n;i++)
		{
			scanf("%d",&x);
			sum+=x;
			f^=(f<<x);
		}
		for(int i=1;i<=sum;i++)
			if(f[i]) ans^=i;
		printf("%d",ans);
    }	
}

int main()
{
    TYC::work();
    return 0;
}
