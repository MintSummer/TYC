#include<bits/stdc++.h>
using namespace std;
const int MAXN=1000011;
int n,m,p,q,fa[MAXN],ans[MAXN];
//并查集fa[i]=j 表示自i往后第一个没有染颜色的是j

inline int find(int a)
{
    return a==fa[a]?a:fa[a]=find(fa[a]);
}

int main()
{
    freopen("TYC.out","w",stdout);
    scanf("%d%d%d%d",&n,&m,&p,&q);
    for(int i=1;i<=n+1;i++) fa[i]=i,ans[i]=0;
    //注意有可能跑到第n + 1个点，于是也要给这个点赋个father
    int l,r,tmp;
    for(int i=m;i;i--)
    {
	l=(i*p+q)%n+1,r=(i*q+p)%n+1;
	if(l>r) swap(l,r);
	for(int j=find(l);j<=r;j=find(j))
	{
	    ans[j]=i;
	    fa[j]=j+1;
	}
    }
    for(int i=1;i<=n-1;i++) printf("%d\n\n",ans[i]);
    printf("%d",ans[n]);
    return 0;
}
